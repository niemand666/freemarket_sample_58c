=render 'shared/main_page_header'

%main.purchased_on_deal.clearfix
  =render 'shared/profile-sidebar'
  .purchased_on_deal__content
    %section.purchased_on_deal__content__container
      %h2.purchased_on_deal__content__container__tab-head 
        購入した商品
      %ul.purchased_on_deal__content__container__list-tabs
        %li.purchased_on_deal__content__container__list-tabs__left
          %h3.purchased_on_deal__content__container__list-tabs__left__text
            =link_to "#", class:'purchased_on_deal__content__container__mypage-tabs__active__h3' do
              取引中
        %li.purchased_on_deal__content__container__list-tabs__right
          %h3.put_up_goods_on_sale__content__container__list-tabs__right__text
            =link_to "#", class:'purchased_on_deal__content__container__mypage-tabs__active__h3' do
              過去の取引

      .purchased_on_deal__content__container__tab-content
        %ul#mypage-tab-transaction-now.purchased_on_deal__content__container__tab-content__mypage-item-list
          %li.purchased_on_deal__content__container__tab-content__mypage-item-list__mypage-item-not-found
            取引中の商品がありません 
            = image_tag '/mercari-logo-gray.svg', alt: "" , class: 'lazyloaded'
    %ul.mypage-history.pager
      %li.pager-prev.pager-cell 
        =link_to "#" do
          %i.icon-arrow-left 
      %li.pager-next.pager-cell 
        =link_to "#" do
          %i.icon-arrow-right

=render 'shared/aside'
=render 'shared/footer'